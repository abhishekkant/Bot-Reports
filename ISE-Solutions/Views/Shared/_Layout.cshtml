            .skin-blue .main-header .navbar .nav > li > a {
                color: #fff;
                WIDTH: 113PX;
            }
              .skin-blue .wrapper { background:#ffffff;
         }
         .content-wrapper, .right-side {background-color: #FFF; }

        </style>*@

   

</head>

<body class="hold-transition skin-blue layout-top-nav">
    <div class="wrapper">

        <!-- Main Header -->
        <header class="main-header">
            <nav class="navbar navbar-static-top">
                <div class="container-wrapper">
                    <a href="../../" class="navbar-brand logoadmin">

                        @*<img src="@Href("~/Content/images/logoAdmin.png")" />*@
                        BOT REPORT PANEL
                    </a>
                    <!-- Navbar Right Menu -->
                    <div class="navbar-custom-menu">
                        <ul class="nav navbar-nav">
                            <li>
                                        @*<a href="#" class="launch-modal" data-toggle="modal" data-target="#modal-video">
                                            <span class="video-link-icon"><i class="fa fa-play"></i></span>
                                            <span class="video-link-text">Excel Demo Video</span>
                                        </a>*@
                            </li>
                            <!-- User Account Menu -->
                            <li class="dropdown user user-menu">
                                <!-- Menu Toggle Button -->
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <!-- The user image in the navbar-->

                                    <img src="~/Image/user2-160x160.jpg" class="user-image" alt="User Image">
                                    <!-- hidden-xs hides the username on small devices so only the image appears. -->
                                    <span class="hidden-xs">@SessionHandler.UserName</span>
                                </a>
                                <!-- The user image in the menu -->
                           
                                <ul class="dropdown-menu">
                              
                                    <li class="user-header">
                                        <img src="~/Image/user2-160x160.jpg" class="img-circle" alt="User Image">
                                        <p>
                                            @SessionHandler.UserName
                                            <small>Member since april 20017</small>
                                        </p>
                                    </li>

                                    <!-- Menu Footer-->
                                    <li class="user-footer">
                                        <div class="pull-left">
                                            <a href="profile.html" class="btn btn-default btn-flat hide">Profile</a>
                                        </div>
                                        @*<div class="pull-right">
                                                <a href="login.html" class="btn btn-default btn-flat">Sign out</a>
                                            </div>*@
                                        <div class="pull-right">
                                            @Html.ActionLink("Sign out", "Logout", "Login", new { area = "" }, new { @class = "btn btn-default btn-flat" })
                                            @*<a href="login.html" class="btn btn-default btn-flat">Sign out</a>*@
                                        </div>

                                    </li>
                                </ul>
                            </li>
                            <!-- Control Sidebar Toggle Button -->

                        </ul>
                    </div>
                    <!-- /.navbar-custom-menu -->

                </div>
                <!-- /.container-fluid -->
            </nav>

            <nav class="nav navbar " style="background-color: #de3468;">
                <div class="container-wrapper">

                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
                            <i class="fa fa-bars"></i>
                        </button>
                    </div>


                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse pull-left" id="navbar-collapse">
                        <ul class="nav navbar-nav">
                            <li id="Dashboard" >
                                @Html.ActionLink("Home", "dashboard", "Home", new { Area = "" }, null)
                            </li>
                            <li id="top10" ><a href="/home/top10">Top 10 Report</a></li>
                            <li  id="addrow"><a href="/home/addrow">Train</a></li>
                            <li id="AzureInsights"><a href="/AzureInsights/index">AzureInsights</a></li>
                        </ul>

                    </div>
                    <!-- /.navbar-collapse -->

                </div>
                <!-- /.container-fluid -->
            </nav>
           
        </header>

        <div class="content-wrapper">


            <!-- Content Header (Page header) -->
            <!-- Main content -->
            @RenderBody()
            <!-- /.content -->
            <div class="modal fade" id="modal-video" tabindex="-1" role="dialog" aria-labelledby="modal-video-label">
                <div class="modal-dialog modal-lg" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                           Excel Upload Demo
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <div class="modal-video">
                                <div class="embed-responsive embed-responsive-16by9">
                                   
                                    <video controls="controls" width="100%" height="100%">
                                        <source src="~/DemoVideo/mesimart_video_excel_final.mp4" type="video/mp4"  />
                                    </video>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            
        </div>

        <!-- /.content-wrapper -->
        <!-- Main Footer -->
        <footer class="main-footer">
            <!-- Default to the left -->
            <strong>Copyright &copy; 2018 </strong> All rights reserved.
        </footer>


    </div>


    <!-- Optionally, you can add Slimscroll and FastClick plugins.
         Both of these plugins are recommended to enhance the
         user experience. Slimscroll is required when using the
         fixed layout. -->
    <script>
     var url = window.location.href;
     //alert(url);
     if (url.indexOf('dashboard') != -1) {
         $('#Dashboard').addClass("active")
     }
     if (url.indexOf('top10') != -1) {
         $('#top10').addClass("active")
     }
     if (url.indexOf('addrow') != -1) {
         $('#addrow').addClass("active")
        }
     if (url.indexOf('AzureInsights') != -1) {
            $('#AzureInsights').addClass("active")
        }
        
    </script>
</body>


</html>
